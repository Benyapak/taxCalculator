  Purpose: Synchronous BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="BPELtaxCalculator"
               targetNamespace="http://xmlns.oracle.com/OBRProject/OBRProject/BPELtaxCalculator"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/OBRProject/OBRProject/BPELtaxCalculator"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ess="http://xmlns.oracle.com/scheduler" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath" xmlns:ns1="http://www.excise.go.th"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/db/OBRProject/OBRProject/db_LR"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/OBRProject/OBRProject/db_TR"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/top/db_TR"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/top/db_LR"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/top/db_LR" location="../Schemas/db_LR_table.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/db/top/db_TR" location="../Schemas/db_TR_table.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <partnerLinks>
    <partnerLink name="RestService" partnerLinkType="client:BPELtaxCalculator" myRole="BPELtaxCalculatorProvider"/>
    <partnerLink name="db_LR" partnerLinkType="ns2:db_LR_plt" partnerRole="db_LR_role"/>
    <partnerLink name="db_TR" partnerLinkType="ns3:db_TR_plt" partnerRole="db_TR_role"/>
  </partnerLinks>
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:BPELtaxCalculatorRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:BPELtaxCalculatorResponseMessage"/>
    <variable name="inputTR"
              element="ns4:db_TRSelect_DutyCode_TypeCode_SubtypeCode_SettypeCode_CalDateInputParameters"/>
    <variable name="outputTR" element="ns4:TimeRateCollection"/>
    <variable name="inputLR" element="ns5:db_LRSelect_offcode_inccode_CalDateInputParameters"/>
    <variable name="outputLR" element="ns5:LocalrateCollection"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BPELtaxCalculator.wsdl) -->
    <receive name="receiveInput" variable="inputVariable" createInstance="yes" partnerLink="RestService"
             portType="client:BPELtaxCalculator" operation="process"/>
    <if name="If1">
      <condition>substring($inputVariable.payload/ns1:ProductInfo/ns1:ProductCode,1,2) &gt; 21 or substring($inputVariable.payload/ns1:ProductInfo/ns1:ProductCode,1,2) = 0</condition>
      <assign name="Assign1">
        <copy>
          <from>"Fail"</from>
          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.payload/ns1:Message</to>
        </copy>
      </assign>
      <else>
        <sequence name="Sequence1">
          <assign name="Assign2">
            <copy>
              <from>$inputVariable.payload/ns1:ProductInfo/ns1:CalDate</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputTR/ns4:CalDate</to>
            </copy>
            <copy>
              <from>substring($inputVariable.payload/ns1:ProductInfo/ns1:ProductCode,1,4)</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputTR/ns4:DutyCode</to>
            </copy>
            <copy>
              <from>substring($inputVariable.payload/ns1:ProductInfo/ns1:ProductCode,5,2)</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputTR/ns4:TypeCode</to>
            </copy>
            <copy>
              <from>substring($inputVariable.payload/ns1:ProductInfo/ns1:ProductCode,7,2)</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputTR/ns4:SubtypeCode</to>
            </copy>
            <copy>
              <from>substring($inputVariable.payload/ns1:ProductInfo/ns1:ProductCode,9,2)</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputTR/ns4:SettypeCode</to>
            </copy>
          </assign>
          <invoke name="Invoke1" inputVariable="inputTR" outputVariable="outputTR"
                  partnerLink="db_TR" portType="ns3:db_TR_ptt" operation="db_TRSelect" bpelx:invokeAsDetail="no"/>
          <assign name="Assign3">
            <copy>
              <from>$inputVariable.payload/ns1:LocalTax/ns1:OffCode</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputLR/ns5:offcode</to>
            </copy>
            <copy>
              <from>$inputVariable.payload/ns1:LocalTax/ns1:IncCode</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputLR/ns5:inccode</to>
            </copy>
            <copy>
              <from>$inputVariable.payload/ns1:ProductInfo/ns1:CalDate</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$inputLR/ns5:CalDate</to>
            </copy>
            <copy>
              <from>$outputTR/ns4:TimeRate/ns4:rateQuantity</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.payload/ns1:QuantityRate</to>
            </copy>
            <copy>
              <from>$outputTR/ns4:TimeRate/ns4:rateValue</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.payload/ns1:ValueRate</to>
            </copy>
          </assign>
          <invoke name="Invoke2" partnerLink="db_LR" portType="ns2:db_LR_ptt"
                  operation="db_LRSelect" inputVariable="inputLR" outputVariable="outputLR" bpelx:invokeAsDetail="no"/>
          <assign name="Assign4">
            <copy>
              <from>$outputLR/ns5:Localrate/ns5:locRate</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.payload/ns1:localTax</to>
            </copy>
          </assign>
        </sequence>
      </else>
    </if>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" variable="outputVariable" partnerLink="RestService" portType="client:BPELtaxCalculator"
           operation="process"/>
  </sequence>
</process>